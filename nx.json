{
  "extends": "@nrwl/workspace/presets/core.json",
  "npmScope": "arwes",
  "affected": {
    "defaultBase": "next"
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build-esm",
          "build-cjs",
          "build-umd"
        ]
      }
    }
  },
  "targetDependencies": {
    "clean": [
      {
        "target": "clean",
        "projects": "dependencies"
      }
    ],
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "build-esm": [
      {
        "target": "build-esm",
        "projects": "dependencies"
      }
    ],
    "build-cjs": [
      {
        "target": "build-cjs",
        "projects": "dependencies"
      }
    ],
    "build-umd": [
      {
        "target": "build-umd",
        "projects": "dependencies"
      }
    ],
    "dev": [
      {
        "target": "build-esm",
        "projects": "dependencies"
      }
    ]
  }
}
